{
  "name": "secp256k1_ios",
  "version": "0.0.1",
  "summary": "Reference sepc256k1 implementation as pod",
  "description": "secp256k1 libraty portable pod",
  "homepage": "https://github.com/shamatar/secp256k1_ios",
  "license": "MIT",
  "authors": {
    "Alex Vlasov": "alex.m.vlasov@gmail.com"
  },
  "source": {
    "git": "https://github.com/shamatar/secp256k1_ios.git",
    "tag": "0.0.1"
  },
  "social_media_url": "https://twitter.com/shamatar",
  "module_name": "secp256k1_ios",
  "platforms": {
    "ios": "9.0",
    "osx": "10.10"
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "$(inherited)",
    "OTHER_CFLAGS": "-DHAVE_CONFIG_H=1",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/../../secp256k1_ios/\""
  },
  "source_files": "secp256k1_ios/libsecp256k1-config.h",
  "private_header_files": "secp256k1_ios/libsecp256k1-config.h",
  "public_header_files": "secp256k1_ios/include/*.h",
  "subspecs": [
    {
      "name": "src",
      "source_files": "secp256k1_ios/src/*.{c,h}",
      "private_header_files": "secp256k1_ios/src/*.h",
      "subspecs": [
        {
          "name": "modules",
          "source_files": "secp256k1_ios/src/modules/*.{c,h}",
          "private_header_files": "secp256k1_ios/src/modules/*.h"
        }
      ]
    },
    {
      "name": "contrib",
      "source_files": "secp256k1_ios/contrib/*.{c,h}",
      "private_header_files": "secp256k1_ios/contrib/*.h"
    },
    {
      "name": "include",
      "source_files": "secp256k1_ios/include/*.{h}",
      "private_header_files": "secp256k1_ios/include/*.h"
    }
  ]
}
